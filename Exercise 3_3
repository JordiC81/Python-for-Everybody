input_score=input('Enter Score ')
try:
    Score=float(input_score)
  
except:
    print('Bad Score')
    quit()

if 0<= Score <=0.9:
    if Score >=0.9: 
        print('A')
    elif  Score >=0.8:
        print('B')
    elif Score >=0.7:
        print('C')
    elif Score >= 0.6:
        print('D')
    elif Score <0.6:
        print('F')
else:
    print('Bad Score')
